# The gitignore files are taken from https://github.com/github/gitignore
# for gradle projects
.gradle
build/

# Ignore Gradle GUI config
gradle-app.setting

# Avoid ignoring Gradle wrapper jar file (.jar files are usually ignored)
!gradle-wrapper.jar

# for Android project
# Built application files
*.apk
*.ap_
# Files for the Dalvik VM
*.dex
# Java class files
*.class
# Generated files
bin/
gen/
# Local configuration file (sdk path, etc)
local.properties
# Log Files
*.log

# Android studio related files
# Signing files
.signing/

# User-specific configurations
.idea
*.iml

# OS-specific files
.DS_Store
.DS_Store?
._*
.Spotlight-V100
.Trashes
ehthumbs.db
Thumbs.db

# Backup files made by editors
# gedit
*~
# vim
*.swp

# Patch files by git
*.patch

